        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C95096"/>
        <obo:IAO_0000115 xml:lang="en">Cellular Neuroscience is the study of neurons at the cellular level. It includes cell morphology and physiological properties such as membrane trafficking, synthesis and transport of proteins, and synaptic plasticity. [Nature https://www.nature.com/subjects/cellular-neuroscience, AL 12.6.2019]</obo:IAO_0000115>
        <obo:IAO_0000412 xml:lang="en">re3data</obo:IAO_0000412>
        <rdfs:label xml:lang="en">Cellular Neuroscience</rdfs:label>
    </owl:Class>
    


    <!-- http://www.fairsharing.org/ontology/subject/SRAO_0000139 -->

    <owl:Class rdf:about="http://www.fairsharing.org/ontology/subject/SRAO_0000139">
        <rdfs:subClassOf rdf:resource="http://edamontology.org/topic_2259"/>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C95096"/>
        <obo:IAO_0000115 xml:lang="en">Systemic Neuroscience is a subdiscipline of Neuroscience and Systems Biology that studies the structure and function of neural circuits and systems. It encompasses a number of areas of study concerned with how nerve cells behave when connected together to form neural pathways, neural circuits, and larger brain networks. [Adapted from Wikipedia https://en.wikipedia.org/wiki/Systems_neuroscience, 3.4.2019]</obo:IAO_0000115>
        <obo:IAO_0000118 xml:lang="en">Systems Neuroscience</obo:IAO_0000118>
        <obo:IAO_0000412 xml:lang="en">re3data</obo:IAO_0000412>
        <subject:SRAO_0000268 xml:lang="en">Systemic Neuroscience, Computational Neuroscience, Behaviour</subject:SRAO_0000268>
        <rdfs:comment xml:lang="en">AL: 9.4.2019 Refactored re3data composite class &apos;Systemic Neuroscience, Computational Neuroscience, Behaviour&apos; into sibling classes &apos;Systemic Neuroscience&apos; and &apos;Computational Neuroscience&apos; according to FAIRsharing curation best practises. The &apos;Behaviour&apos; aspect of the original class name is now superceded by &apos;Biological Psychology&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">Systemic Neuroscience</rdfs:label>
    </owl:Class>
    


