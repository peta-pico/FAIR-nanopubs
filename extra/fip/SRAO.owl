    <owl:Class rdf:about="http://www.fairsharing.org/ontology/subject/SRAO_0000140">
        <rdfs:subClassOf rdf:resource="http://edamontology.org/topic_3304"/>
        <obo:IAO_0000115 xml:lang="en">Comparative Neurobiology is the study of how nervous systems work by comparing the neurobiology of multiple species. [Adapted from https://le.ac.uk/modules/bs3064, AL 12.6.2019]</obo:IAO_0000115>
        <obo:IAO_0000412 xml:lang="en">re3data</obo:IAO_0000412>
        <rdfs:label xml:lang="en">Comparative Neurobiology</rdfs:label>
    </owl:Class>
    


