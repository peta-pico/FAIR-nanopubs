        <obo:IAO_0000412 xml:lang="en">NCIT</obo:IAO_0000412>
        <subject:SRAO_0000276>Proteomics</subject:SRAO_0000276>
        <subject:SRAO_0000276>proteomics</subject:SRAO_0000276>
        <rdfs:comment xml:lang="en">AL 11.6.2019: This class has been refactored from EDAM to NCIT to better align with FAIRsharing curation guidelines.</rdfs:comment>
        <rdfs:label xml:lang="en">Proteomics</rdfs:label>
    </owl:Class>
    


