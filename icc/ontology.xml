    <!-- http://xmlns.com/foaf/0.1/member -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/member"/>
    


    <!-- https://w3id.org/fair/icc/terms/chosen-resource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/chosen-resource">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChoiceDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Resource"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/chosen-resource"/>
        <rdfs:label xml:lang="en">chosen resource</rdfs:label>
        <skos:definition xml:lang="en">Connects ChoiceDeclaration to the Resource that was chosen through the declaration</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/declared-by -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/declared-by">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Community"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/declared-by"/>
        <rdfs:label xml:lang="en">declared by</rdfs:label>
        <skos:definition xml:lang="en">Connects a Declaration to the Community that made the declaration</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/explains-principle -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/explains-principle">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Explanation"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/explains-principle"/>
        <rdfs:label xml:lang="en">explains principle</rdfs:label>
        <skos:definition xml:lang="en">Links an explanation to the FAIR (sub)principle it explains</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/had-challenge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/had-challenge">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChoiceDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/ChallengeDeclaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/had-challenge"/>
        <rdfs:label xml:lang="en">has open challenge</rdfs:label>
        <skos:definition xml:lang="en">Connects a ChoiceDeclaration to a ChallengeDeclaration that describes a challenge that had to be met before the choice could be made</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/refers-to-challenge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/refers-to-challenge">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChallengeDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Challenge"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/refers-to-challenge"/>
        <rdfs:label xml:lang="en">refers to challenge</rdfs:label>
        <skos:definition xml:lang="en">Connects a ChallengeDeclaration to the Challenge it is derived from</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/refers-to-choice -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/refers-to-choice">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChoiceDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Choice"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/refers-to-choice"/>
        <rdfs:label xml:lang="en">refers to choice</rdfs:label>
        <skos:definition xml:lang="en">Connects a ChoiceDeclaration to the Choice it is derived from</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/refers-to-principle -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/refers-to-principle">
        <rdfs:range rdf:resource="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/refers-to-principle"/>
        <rdfs:label xml:lang="en">refers to principle</rdfs:label>
        <skos:definition xml:lang="en">Connects an entity (for example a choice or challenge) to a FAIR (sub)principle it refers to</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/related-challenge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/related-challenge">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Choice"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Challenge"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/related-challenge"/>
        <rdfs:label xml:lang="en">related challenge</rdfs:label>
        <skos:definition xml:lang="en">Connects a Choice to a Challenge that some communities might face before they can make the given kind of choice</skos:definition>
    </owl:ObjectProperty>
    


