    <!-- http://xmlns.com/foaf/0.1/Group -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Group"/>
    


    <!-- https://w3id.org/fair/icc/terms/Challenge -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Challenge">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Challenge"/>
        <rdfs:label xml:lang="en">Challenge</rdfs:label>
        <skos:definition xml:lang="en">The kind of challenge that some communities face if the lack of existing resources doesn&apos;t allow them to make a choice yet</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/ChallengeDeclaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/ChallengeDeclaration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/ChallengeDeclaration"/>
        <rdfs:label xml:lang="en">Challenge Declaration</rdfs:label>
        <skos:definition xml:lang="en">The expression of a community of a challenge they accepted</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Choice -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Choice">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Choice"/>
        <rdfs:label xml:lang="en">Choice</rdfs:label>
        <skos:definition xml:lang="en">The kind of choice communities have to make (can be expressed as a question)</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/ChoiceDeclaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/ChoiceDeclaration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/ChoiceDeclaration"/>
        <rdfs:label xml:lang="en">Choice Declaration</rdfs:label>
        <skos:definition xml:lang="en">The expression of a community of their choice made (corresponds to an answer to a question)</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Community -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Community">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Community"/>
        <rdfs:label xml:lang="en">Community</rdfs:label>
        <skos:definition xml:lang="en">A non-empty set of people and/or organizations that form a self-declared community with the aim to implement the FAIR principles for their fields of interest</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Declaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Declaration">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Declaration"/>
        <rdfs:label xml:lang="en">Declaration</rdfs:label>
        <skos:definition xml:lang="en">Something that is either a ChoiceDeclaration or a ChallengeDeclaration</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Explanation -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Explanation">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Explanation"/>
        <rdfs:label xml:lang="en">Explanation</rdfs:label>
        <skos:definition xml:lang="en">An explanation of a FAIR (sub)principle</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Resource -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Resource">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Resource"/>
        <rdfs:label xml:lang="en">Resource</rdfs:label>
        <skos:definition xml:lang="en">An artifact or service that can contribute to the implementation of the FAIR principles</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple -->


    <owl:Class rdf:about="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple">
        <rdfs:label>FAIR Principle or Sub-Principle</rdfs:label>
    </owl:Class>
    


