    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://xmlns.com/foaf/0.1/member -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/member"/>
    


    <!-- https://w3id.org/fair/icc/terms/chosen-resource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/chosen-resource">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChoiceDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Resource"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/chosen-resource"/>
        <rdfs:label xml:lang="en">chosen resource</rdfs:label>
        <skos:definition xml:lang="en">Connects ChoiceDeclaration to the Resource that was chosen through the declaration</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/declared-by -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/declared-by">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Community"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/declared-by"/>
        <rdfs:label xml:lang="en">declared by</rdfs:label>
        <skos:definition xml:lang="en">Connects a Declaration to the Community that made the declaration</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/explains-principle -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/explains-principle">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Explanation"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/explains-principle"/>
        <rdfs:label xml:lang="en">explains principle</rdfs:label>
        <skos:definition xml:lang="en">Links an explanation to the FAIR (sub)principle it explains</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/has-open-challenge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/has-open-challenge">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/ChoiceDeclaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/ChallengeDeclaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/has-open-challenge"/>
        <rdfs:label xml:lang="en">has open challenge</rdfs:label>
        <skos:definition xml:lang="en">Connects a ChoiceDeclaration that doesn&apos;t have a chosen resource to a ChallengeDeclaration that describes a challenge that has to be met before the community can choose a resource</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/refers-to-consideration -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/refers-to-consideration">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Consideration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/refers-to-consideration"/>
        <rdfs:label xml:lang="en">refers to consideration</rdfs:label>
        <skos:definition xml:lang="en">Connects a Declaration to the Consideration it is derived from</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/refers-to-principle -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/refers-to-principle">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Consideration"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/refers-to-principle"/>
        <rdfs:label xml:lang="en">refers to principle</rdfs:label>
        <skos:definition xml:lang="en">Connects a Consideration to a FAIR (sub)principle it refers to</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fair/icc/terms/related-challenge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fair/icc/terms/related-challenge">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Choice"/>
        <rdfs:range rdf:resource="https://w3id.org/fair/icc/terms/Challenge"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/related-challenge"/>
        <rdfs:label xml:lang="en">related challenge</rdfs:label>
        <skos:definition xml:lang="en">Connects a Choice to a Challenge that some communities might face before they can make the given kind of choice</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://xmlns.com/foaf/0.1/Group -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Group"/>
    


    <!-- https://w3id.org/fair/icc/terms/Challenge -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Challenge">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Consideration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Challenge"/>
        <rdfs:label xml:lang="en">Challenge</rdfs:label>
        <skos:definition xml:lang="en">The kind of challenge that some communities face if the lack of existing resources doesn&apos;t allow them to make a choice yet</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/ChallengeDeclaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/ChallengeDeclaration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/ChallengeDeclaration"/>
        <rdfs:label xml:lang="en">Challenge Declaration</rdfs:label>
        <skos:definition xml:lang="en">The expression of a community of a challenge they accepted</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Choice -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Choice">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Consideration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Choice"/>
        <rdfs:label xml:lang="en">Choice</rdfs:label>
        <skos:definition xml:lang="en">The kind of choice communities have to make (can be expressed as a question)</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/ChoiceDeclaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/ChoiceDeclaration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fair/icc/terms/Declaration"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/ChoiceDeclaration"/>
        <rdfs:label xml:lang="en">Choice Declaration</rdfs:label>
        <skos:definition xml:lang="en">The expression of a community of their choice made (corresponds to an answer to a question)</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Community -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Community">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Community"/>
        <rdfs:label xml:lang="en">Community</rdfs:label>
        <skos:definition xml:lang="en">A non-empty set of people and/or organizations that form a self-declared community with the aim to implement the FAIR principles for their fields of interest</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Consideration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Consideration">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Consideration"/>
        <rdfs:label xml:lang="en">Consideration</rdfs:label>
        <skos:definition xml:lang="en">Something that is either a Choice or a Challenge</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Declaration -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Declaration">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Declaration"/>
        <rdfs:label xml:lang="en">Declaration</rdfs:label>
        <skos:definition xml:lang="en">Something that is either a ChoiceDeclaration or a ChallengeDeclaration</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Explanation -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Explanation">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Explanation"/>
        <rdfs:label xml:lang="en">Explanation</rdfs:label>
        <skos:definition xml:lang="en">An explanation of a FAIR (sub)principle</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/icc/terms/Resource -->


    <owl:Class rdf:about="https://w3id.org/fair/icc/terms/Resource">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/Resource"/>
        <rdfs:label xml:lang="en">Resource</rdfs:label>
        <skos:definition xml:lang="en">An artifact or service that can contribute to the implementation of the FAIR principles</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple -->


    <owl:Class rdf:about="https://w3id.org/fair/principles/terms/FAIR-Principle-or-SubPrinciple">
        <rdfs:label>FAIR Principle or Sub-Principle</rdfs:label>
    </owl:Class>
    


