    <!-- https://w3id.org/fair/icc/terms/implementation-considerations -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/fair/icc/terms/implementation-considerations">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Explanation"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/implementation-considerations"/>
        <rdfs:label xml:lang="en">implementation consideration</rdfs:label>
        <skos:definition xml:lang="en">Explains considerations for communities who are about to implement the given principle.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fair/icc/terms/implementation-examples -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/fair/icc/terms/implementation-examples">
        <rdfs:domain rdf:resource="https://w3id.org/fair/icc/terms/Explanation"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/fair/icc/latest/implementation-examples"/>
        <rdfs:label xml:lang="en">implementation examples</rdfs:label>
        <skos:definition xml:lang="en">Describes examples around the implementation of the given principle.</skos:definition>
    </owl:DatatypeProperty>
    


