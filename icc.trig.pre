:assertion {
  fair:F1 dct:description "Principle F1 states that resources, i.e. data and metadata, should be assigned a globally unique and persistent identifier (GUPID). ..." ;
    icc:hasExample :ex1 , :ex2 ;
    icc:hasCounterExample :cex1 .
  :ex1 dct:description "Universally unique identifier (UUID)" ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Universally_unique_identifier> .
  :ex2 rdf:value "https://orcid.org/0000-0001-8888-635X" ;
    dct:description "One particular person on planet earth with his globally unique and persistent identifier via the ORCID identifier scheme" ;
    rdfs:seeAlso <https://orcid.org/> .
  :cex1 rdf:value "163483" ;
    dct:description "The number 163483 refers to the undergraduate student ID of Mark Wilkinson, the NCBI gi number for a bovine protease, and a part number for a Singer sewing machine. Hence, this is a poor example of globally unique and persistent identifier." .
}

:provenance {
  :assertion pav:authoredBy
    orcid:0000-0001-8888-635X ,
    orcid:0000-0003-4818-2360 .
}

